		{
			"name": "APIContract",
			"item": [
				{
					"name": "OpportunitiesContract",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"let resp=pm.response;\r",
									"let jresp=resp.json();\r",
									"//validate 200 OK Status code\r",
									"\r",
									"pm.test(\"Validate 200 OK Status Code\",function(){\r",
									"    pm.expect(resp.code).to.eql(200);\r",
									"});\r",
									"// Validate ok:true\r",
									"\r",
									"pm.test(\"Validate ok true in response\",function(){\r",
									"\r",
									"    pm.expect(jresp.ok).to.eql(true);\r",
									"});\r",
									"\r",
									"//validate tf:H1\r",
									"// validate tf:H1 (top-level + per-row)\r",
									"pm.test(\"Validate tf should be H1 (root + rows)\", function () {\r",
									"  const jresp = pm.response.json();\r",
									"\r",
									"  // 1) root tf\r",
									"  pm.expect(jresp).to.have.property(\"tf\");\r",
									"  pm.expect(String(jresp.tf).toUpperCase()).to.eql(\"H1\");\r",
									"\r",
									"  // 2) rows tf (only if row.tf exists)\r",
									"  pm.expect(jresp.rows).to.be.an(\"array\");\r",
									"  jresp.rows.forEach((row, i) => {\r",
									"    if (row.tf !== undefined && row.tf !== null) {\r",
									"      pm.expect(String(row.tf).toUpperCase(), `row[${i}].tf`).to.eql(\"H1\");\r",
									"    }\r",
									"  });\r",
									"});\r",
									"\r",
									"\r",
									"// every rows has symbol exist\r",
									"\r",
									"pm.test(\"Validate Symbol exist\",function(){\r",
									"    pm.expect(jresp.rows).to.be.an(\"array\");\r",
									"    jresp.rows.forEach((row,i)=>{\r",
									"  \r",
									"      pm.expect(row, `row[${i}]`).to.have.property(\"symbol\");\r",
									"      pm.expect(row.symbol, `row[${i}].symbol`).to.be.a(\"string\").and.not.be.empty;\r",
									"\t});\r",
									"});\r",
									"\r",
									"// if decision=BUY, zone must exist\r",
									"pm.test(\"Validate zone exists for BUY decision\", function () {\r",
									"  const jresp = pm.response.json();\r",
									"\r",
									"  pm.expect(jresp.rows).to.be.an(\"array\");\r",
									"\r",
									"  jresp.rows.forEach((row, i) => {\r",
									"    // decision should exist (optional but fine)\r",
									"    pm.expect(row, `row[${i}]`).to.have.property(\"decision\");\r",
									"\r",
									"    // only validate zone for BUY rows\r",
									"    if (String(row.decision || \"\").toUpperCase() === \"BUY\") {\r",
									"      // zone might be at row.zone OR row.entry_gate.zone depending on endpoint\r",
									"      const zone = row.zone || (row.entry_gate && row.entry_gate.zone);\r",
									"\r",
									"      pm.expect(zone, `row[${i}] zone for BUY`).to.be.an(\"object\");\r",
									"      pm.expect(zone, `row[${i}] zone.level`).to.have.property(\"level\");\r",
									"      pm.expect(zone.level, `row[${i}] zone.level`).to.be.a(\"number\");\r",
									"\r",
									"      // optional but usually useful if you use zone bands\r",
									"      if (zone.low !== undefined || zone.high !== undefined) {\r",
									"        pm.expect(zone.low, `row[${i}] zone.low`).to.be.a(\"number\");\r",
									"        pm.expect(zone.high, `row[${i}] zone.high`).to.be.a(\"number\");\r",
									"      }\r",
									"    }\r",
									"  });\r",
									"});\r",
									"\r",
									"\r",
									"\r",
									"\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const csv = pm.collectionVariables.get(\"symbols_csv\") || \"\";\r",
									"const arr = csv.split(\",\").map(s => s.trim()).filter(Boolean);\r",
									"\r",
									"let i = parseInt(pm.collectionVariables.get(\"sym_i\") || \"0\", 10);\r",
									"\r",
									"if (i >= arr.length) {\r",
									"  postman.setNextRequest(null); // stop\r",
									"} else {\r",
									"  pm.collectionVariables.set(\"sym\", arr[i]); // current symbol\r",
									"}\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
