				{
					"name": "Opportunities-loop",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"const sym = pm.variables.get(\"sym\") || \"NA\";\r",
									"const j = pm.response.json();\r",
									"\r",
									"pm.test(`[${sym}] 200 OK`, () => pm.response.to.have.status(200));\r",
									"pm.test(`[${sym}] ok true`, () => pm.expect(j.ok).to.eql(true));\r",
									"pm.test(`[${sym}] tf is H1`, () => pm.expect(String(j.tf).toUpperCase()).to.eql(\"H1\"));\r",
									"pm.test(`[${sym}] rows array exists`, () => pm.expect(j.rows).to.be.an(\"array\"));\r",
									"\r",
									"const hasRows = Array.isArray(j.rows) && j.rows.length > 0;\r",
									"\r",
									"pm.test(`[${sym}] EXPECT rows > 0`, function () {\r",
									"  pm.expect(j.rows.length, `No rows returned for ${sym}`).to.be.above(0);\r",
									"});\r",
									"\r",
									"pm.test(`[${sym}] Every row has symbol`, function () {\r",
									"  pm.expect(hasRows, `No data for ${sym}, cannot validate symbols`).to.eql(true);\r",
									"  j.rows.forEach((row, i) => {\r",
									"    pm.expect(row, `row[${i}]`).to.have.property(\"symbol\");\r",
									"    pm.expect(row.symbol, `row[${i}].symbol`).to.be.a(\"string\").and.not.be.empty;\r",
									"  });\r",
									"});\r",
									"\r",
									"pm.test(`[${sym}] BUY rows must have zone`, function () {\r",
									"  pm.expect(hasRows, `No data for ${sym}, cannot validate zones`).to.eql(true);\r",
									"  j.rows.forEach((row, i) => {\r",
									"    if (String(row.decision || \"\").toUpperCase() === \"BUY\") {\r",
									"      const zone = row.zone || (row.entry_gate && row.entry_gate.zone);\r",
									"      pm.expect(zone, `row[${i}] zone for BUY`).to.be.an(\"object\");\r",
									"      pm.expect(zone).to.have.property(\"level\");\r",
									"    }\r",
									"  });\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
