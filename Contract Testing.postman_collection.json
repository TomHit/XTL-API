				{
					"name": "compare_ohlc (USDJPY H1)",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// compare_ohlc: ignore failures, only log\r",
									"\r",
									"if (!pm.response || pm.response.code >= 400) {\r",
									"  pm.test(\"compare_ohlc ignored (http error)\", function () {\r",
									"    pm.expect(true).to.eql(true);\r",
									"  });\r",
									"  console.log(\"compare_ohlc ignored. code=\", pm.response ? pm.response.code : \"no_response\");\r",
									"  return;\r",
									"}\r",
									"\r",
									"let j = {};\r",
									"try {\r",
									"  j = pm.response.json();\r",
									"} catch (e) {\r",
									"  pm.test(\"compare_ohlc ignored (bad json)\", function () {\r",
									"    pm.expect(true).to.eql(true);\r",
									"  });\r",
									"  console.log(\"compare_ohlc ignored. bad json:\", e.message);\r",
									"  return;\r",
									"}\r",
									"\r",
									"// If your API returns {ok:false} etc, ignore it too:\r",
									"if (j && j.ok === false) {\r",
									"  pm.test(\"compare_ohlc ignored (ok=false)\", function () {\r",
									"    pm.expect(true).to.eql(true);\r",
									"  });\r",
									"  console.log(\"compare_ohlc ok=false ignored. payload=\", j);\r",
									"  return;\r",
									"}\r",
									"\r",
									"pm.test(\"compare_ohlc ok (informational)\", function () {\r",
									"  pm.expect(true).to.eql(true);\r",
									"});\r",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						}
					],
